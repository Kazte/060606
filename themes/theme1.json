{
	"$schema": "vscode://schemas/color-theme",
	"type": "dark",
	"colors": {
		/* editor */
		"editor.background": "#000000",
		"editor.foreground": "#cccccc",
		"editorIndentGuide.background": "#0A0A0A",
		"editorIndentGuide.activeBackground": "#0A0A0A",
		"editor.selectionHighlightBackground": "#0A0A0A",
		"editorWidget.background": "#0A0A0A",
		"editor.lineHighlightBorder": "#0A0A0A",
		"editorLineNumber.foreground": "#cccccc",
		"editor.selectionBackground": "#141414",
		"editor.inactiveSelectionBackground": "#141414",
		/* side bar */
		"sideBarTitle.foreground": "#cccccc",
		"sideBar.background": "#000000",
		"sideBarSectionHeader.background": "#141414",
		/* activity bar */
		"activityBar.background": "#0A0A0A",
		"activityBarBadge.foreground": "#000000",
		"activityBarBadge.background": "#ffffff",
		/* input */
		"input.placeholderForeground": "#cccccc",
		"input.background": "#141414",
		/* menu */
		"menu.background": "#141414",
		"menu.foreground": "#cccccc",
		"menu.selectionBackground": "#1E1E1E",
		/* status bar */
		"statusBar.foreground": "#ffffff",
		"statusBar.background": "#0a0a0a",
		"statusBarItem.hoverBackground": "#141414",
		"statusBar.border": "#ff000000",
		"statusBar.debuggingBackground": "#0a0a0a",
		"statusBar.debuggingForeground": "#ffffff",
		"statusBar.debuggingBorder": "#ff000000",
		"statusBar.noFolderBackground": "#0a0a0a",
		"statusBar.noFolderForeground": "#ffffff",
		"statusBar.noFolderBorder": "#ff000000",
		"statusBarItem.remoteBackground": "#0a0a0a",
		"statusBarItem.remoteForeground": "#ffffff",
		/* title bar */
		"titleBar.activeBackground": "#0A0A0A",
		/* button */
		"button.background": "#141414",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#282828",
		"button.secondaryForeground": "#ffffff",
		"button.secondaryBackground": "#141414",
		"button.secondaryHoverBackground": "#282828",
		"checkbox.background": "#141414",
		/* focus */
		"focusBorder": "#0A0A0A",
		/* tab */
		"editorGroupHeader.tabsBackground": "#000000",
		"tab.activeBackground": "#0A0A0A",
		"tab.inactiveBackground": "#141414",
		"tab.border": "#0A0A0A",
		/* dropdown */
		"dropdown.background": "#141414",
		/* progress bar */
		"progressBar.background": "#ffffff",
		/* list */
		"list.activeSelectionBackground": "#141414",
		"list.inactiveSelectionBackground": "#141414",
		"list.hoverBackground": "#1E1E1E",
		"list.focusBackground": "#1E1E1E",
		"list.highlightForeground": "#ffffff",
		"tree.indentGuidesStroke": "#00000000",
		/* notification */
		"notifications.foreground": "#ffffff",
		"notifications.background": "#1e1e1e",
		"notificationToast.border": "#00000000",
		"notificationsErrorIcon.foreground": "#ffffff",
		"notificationsWarningIcon.foreground": "#ffffff",
		"notificationsInfoIcon.foreground": "#ffffff",
		"notificationCenter.border": "#0a0a0a",
		"notificationCenterHeader.foreground": "#ffffff",
		"notificationCenterHeader.background": "#0a0a0a",
		"notifications.border": "#0a0a0a",
		/* scrollbar */
		"scrollbarSlider.hoverBackground": "#0A0A0A",
		"scrollbarSlider.activeBackground": "#0A0A0A",
		"scrollbarSlider.background": "#0A0A0A",
		/* terminal */
		"panel.border": "#0A0A0A",
		/* badge */
		"badge.background": "#ffffff",
		"badge.foreground": "#000000",
		/* suggest widget */
		"editorSuggestWidget.background": "#0A0A0A",
		"editorSuggestWidget.border": "#0A0A0A",
		"editorSuggestWidget.foreground": "#cccccc",
		"editorSuggestWidget.highlightForeground": "#ffffff",
		"editorSuggestWidget.selectedBackground": "#141414",
		/* hover widget */
		"editorHoverWidget.foreground": "#cccccc",
		"editorHoverWidget.background": "#0A0A0A",
		"editorHoverWidget.border": "#0A0A0A",
		/* changed modified lines */
		"editorGutter.modifiedBackground": "#16A3DF",
		"editorGutter.addedBackground": "#28A443",
		"editorGutter.deletedBackground": "#CC001B",
		/* editor underlines */
		"editorError.foreground": "#CC001B",
		"editorWarning.foreground": "#F5AB00",
		"editorInfo.foreground": "#16A3DF",
		/* git */
		"gitDecoration.untrackedResourceForeground": "#28A443",
		"gitDecoration.deletedResourceForeground": "#CC001B",
		"gitDecoration.modifiedResourceForeground": "#16A3DF",
		"gitDecoration.ignoredResourceForeground": "#696969"
	},
	"tokenColors": [
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "comment",
			"settings": {
				"foreground": "#a19b8f"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#D4D4D4"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language.null",
				"variable.language.this",
				"support.type.primitive",
				"support.function",
				"support.variable.dom",
				"support.variable.property",
				"support.variable.property",

				// CSS
				"meta.property-name",
				"meta.property-value",

				// Handlebars
				"support.constant.handlebars"
			],
			"settings": {
				"foreground": "#77BFF7"
			}
		},
		{
			"scope": ["keyword", "storage.modifier", "storage.type", "variable.parameter"],
			"settings": {
				"foreground": "#eb7e81"
			}
		},
		{
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"meta.function-call",

				// HTML/XML
				"entity.other.attribute-name",

				// Shell
				"entity.name.function.shell"
			],
			"settings": {
				"foreground": "#bb97e7"
			}
		},
		{
			"scope": ["entity.name.tag"],
			"settings": {
				"foreground": "#66B5F6"
			}
		}
	]
}
